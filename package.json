{
  "name": "ibex27",
  "version": "1.0.0",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "USER",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "build:web": "vite build -c vite.web.config.ts",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push:pg",
    "db:push-direct": "npx tsx scripts/apply-schema-direct.ts",
    "db:studio": "drizzle-kit studio",
    "db:prepare": "npx tsx scripts/prepare-data-migration.ts",
    "db:check-data": "npx tsx scripts/check-data.ts",
    "db:check-tables": "npx tsx scripts/check-tables.ts",
    "db:check-users": "npx tsx scripts/check-test-users.ts",
    "db:seed-users": "npx tsx scripts/seed-test-users.ts"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@react-pdf/renderer": "^4.3.1",
    "@types/qrcode": "^1.5.6",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.5.0",
    "clsx": "^2.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.30.10",
    "fontkit": "^2.0.4",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.562.0",
    "papaparse": "^5.5.3",
    "pg": "^8.11.5",
    "postgres": "^3.4.4",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.11.0",
    "recharts": "^3.6.0",
    "tailwind-merge": "^2.2.2"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^18.19.31",
    "@types/papaparse": "^5.5.2",
    "@types/pg": "^8.11.5",
    "@types/react": "^18.2.75",
    "@types/react-dom": "^18.2.24",
    "@vercel/node": "^5.5.16",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "drizzle-kit": "^0.20.14",
    "electron": "^30.5.1",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.2.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.7.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.8"
  }
}
